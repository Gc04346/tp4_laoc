wave create -driver freeze -pattern clock -initialvalue 0 -period 50ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/Snooping/clock
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 1000ps sim:/Snooping/clear
wave modify -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 10ps Edit:/Snooping/clear
add wave -position end  sim:/Snooping/inst
add wave -position end  sim:/Snooping/instr_out
add wave -position end  sim:/Snooping/passo
add wave -position end  sim:/Snooping/pc
run -all
write format wave -window .main_pane.wave.interior.cs.body.pw.wf C:/Users/Daniel.BATCOMPUTER/Desktop/Pratica4_DanielSantana_IsaqueFernando/CPU/simulation/modelsim/snooping_mem_instr.do
