add wave -position 2 -format Logic -height 17 -editable 2 Edit:/Snooping/clear
wave modify -driver freeze -pattern clock -initialvalue St0 -period 50ps -dutycycle 50 -starttime 0ps -endtime 10000ps Edit:/Snooping/clock
wave modify -driver freeze -pattern constant -value St0 -starttime 10ps -endtime 10000ps Edit:/Snooping/clear
write format wave -window .main_pane.wave.interior.cs.body.pw.wf C:/Users/Isaqu/Desktop/TP4_Laoc/Parte_2/Snooping/simulation/modelsim/simulacao_top.do
wave editwrite -file C:/Users/Isaqu/Desktop/TP4_Laoc/Parte_2/Snooping/simulation/modelsim/simulacao_top.do -append
run -all
vlog -reportprogress 300 -work work C:/Users/Isaqu/Desktop/TP4_Laoc/Parte_2/Snooping/Bus_arbiter.v
