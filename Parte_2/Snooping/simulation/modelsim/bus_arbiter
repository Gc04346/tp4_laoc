onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /Bus_arbiter/clear
add wave -noupdate /Bus_arbiter/clock
add wave -noupdate /Bus_arbiter/hab_bus
add wave -noupdate -radix unsigned /Bus_arbiter/bus_in
add wave -noupdate /Bus_arbiter/bus
TreeUpdate [SetDefaultTree]
WaveRestoreCursors {{Cursor 1} {0 ps} 0}
quietly wave cursor active 0
configure wave -namecolwidth 150
configure wave -valuecolwidth 100
configure wave -justifyvalue left
configure wave -signalnamewidth 0
configure wave -snapdistance 10
configure wave -datasetprefix 0
configure wave -rowmargin 4
configure wave -childrowmargin 2
configure wave -gridoffset 0
configure wave -gridperiod 1
configure wave -griddelta 40
configure wave -timeline 0
configure wave -timelineunits ps
update
WaveRestoreZoom {0 ps} {1 ns}
view wave 
wave clipboard store
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 1000ps sim:/Bus_arbiter/clear 
wave modify -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 10ps Edit:/Bus_arbiter/clear 
wave create -driver freeze -pattern clock -initialvalue HiZ -period 50ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/Bus_arbiter/clock 
wave create -driver freeze -pattern constant -value 0 -starttime 0ps -endtime 1000ps sim:/Bus_arbiter/hab_bus 
wave create -driver freeze -pattern clock -initialvalue 0 -period 200ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/Bus_arbiter/hab_bus 
wave create -driver freeze -pattern counter -startvalue 0000000000 -endvalue 1111111111 -type Range -direction Up -period 50ps -step 1 -repeat forever -range 9 0 -starttime 0ps -endtime 1000ps sim:/Bus_arbiter/bus_in 
WaveExpandAll -1
WaveCollapseAll -1
wave clipboard restore
